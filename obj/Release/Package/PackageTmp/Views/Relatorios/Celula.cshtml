
@{
    ViewBag.Title = "Celula";
}



<div class="panel panel-default">
    <div class="panel-heading">
        <h2 align="center">Relatório Célula </h2>

    </div>
    <div class="panel-body">
        <form method="post" action="/Relatorios/Pdf" id="frmVenda" class="form-horizontal">

            <div class="form-group">
                <label for="dataInicial" class="col-sm-2 control-label">Nome do Lider / Bairro / Dia da Semana</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="Pesquisa" name="Pesquisa" required>

                </div>

            </div>


            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-success">Gerar Relatório</button>

                </div>

            </div>

        </form>

    </div>

</div>